<div class="col-md-4 organization-section">
  <%= link_to organization, action: 'go' do %>
    <div class="organization-box">
      <div class="organization">
      <div class="caption">
        <h3 class="organization-title"><%= organization.name.truncate(30) %></h3>
      </div>
      <div class="thumbnail">
        <img class="picture" src="<%= organization.display_image %>" alt="">
        <div class="row">
        </div>
      </div>
      </div>
    </div>
  <% end %>
</div>